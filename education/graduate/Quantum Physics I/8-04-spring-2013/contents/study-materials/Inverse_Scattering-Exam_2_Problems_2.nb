Cell[BoxData[
 RowBox[{
  RowBox[{"Numerator", "[", 
   RowBox[{
    RowBox[{"Det", "[", 
     RowBox[{
      RowBox[{"S", "/.", 
       RowBox[{"{", 
        RowBox[{"\[Eta]", "\[Rule]", 
         RowBox[{
          RowBox[{"-", "\[ImaginaryI]"}], " ", "\[Beta]"}]}], "}"}]}], "//", 
      "TrigToExp"}], "]"}], "//", "Simplify"}], "]"}], "\[Equal]", 
  "0"}]], "Input",
 CellChangeTimes->{{3.575661639243075*^9, 3.5756618209331007`*^9}, {
  3.575661914567627*^9, 3.575661915078887*^9}, {3.5756619592657433`*^9, 
  3.575662021735277*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"(", 
     RowBox[{"1", "-", 
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{
        RowBox[{"-", "2"}], " ", "\[Beta]"}]]}], ")"}], " ", 
    SuperscriptBox["g", "2"]}], "-", 
   SuperscriptBox["\[Beta]", "2"]}], "\[Equal]", "0"}]], "Output",
 CellChangeTimes->{{3.575661666069274*^9, 3.5756616804969397`*^9}, {
   3.575661735542755*^9, 3.575661746529375*^9}, {3.575661789021646*^9, 
   3.575661821741407*^9}, 3.575661916809486*^9, {3.575661959855692*^9, 
   3.575662022115512*^9}, 3.575664160420575*^9, 3.575680897436905*^9, 
   3.5765400164354553`*^9}]
}, Open  ]],

Cell[TextData[{
 "Which, again, is the same condition.  Good.  The lesson here is that we \
could use either sign in the analytic continuation ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Eta]", "\[Rule]", 
    RowBox[{
     RowBox[{"\[PlusMinus]", " ", "\[ImaginaryI]"}], " ", "\[Beta]"}]}], 
   TraditionalForm]],
  FormatType->"TraditionalForm"],
 ", but which sign we choose determines which of ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{"A", ",", "D"}], "}"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " or ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{"B", ",", "C"}], "}"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " must vanish for normalizability, and thus whether ",
 Cell[BoxData[
  FormBox["S", TraditionalForm]],
  FormatType->"TraditionalForm"],
 " must have a pole or a zero."
}], "Text",
 CellChangeTimes->{{3.575662026948452*^9, 3.575662057993664*^9}, {
  3.575662261786298*^9, 3.575662453784663*^9}}]
}, Open  ]]

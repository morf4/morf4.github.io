<Grid 
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:d="http://schemas.microsoft.com/expression/interactivedesigner/2006"
	mc:Ignorable="d" 
	Background="sc#1, 1, 1, 1" 
	x:Name="DocumentRoot"
	x:Class="WoodgroveFinanceHost.StockHost" 
xmlns:System="clr-namespace:System;assembly=mscorlib"
  Loaded="OnLoaded" 
	 xmlns:WoodgroveFinanceStockChartFrame="clr-namespace:WoodgroveFinanceStockChartFrame;assembly=WoodgroveFinanceStockChartFrame"  >

  <Grid.Resources>





    <ResourceDictionary>
      <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="StockHostResourceDictionary.xaml"/>
      </ResourceDictionary.MergedDictionaries>

      <WoodgroveFinanceStockChartFrame:StockImageConverter x:Key="stockImageConverter"  />
      <WoodgroveFinanceStockChartFrame:IsNegativeConverter x:Key="IsNegativeConverterReference" />

      <ControlTemplate x:Key="CustomExpander2" TargetType="{x:Type Expander}">


        <Border Background="{TemplateBinding Background}" x:Name="Border"  >
          <DockPanel x:Name="DockPanel">
            <ToggleButton IsChecked="{Binding Path=IsExpanded,Mode=TwoWay,RelativeSource={RelativeSource TemplatedParent}}" 
                          DockPanel.Dock="Top" x:Name="HeaderSite" 
                          Content="{TemplateBinding Header}" >
              <ToggleButton.Style>
                <Style TargetType="{x:Type ToggleButton}">
                  <Setter Property="Template">
                    <Setter.Value>
                      <ControlTemplate TargetType="{x:Type ToggleButton}">
                        <Border x:Name="Border" Padding="{TemplateBinding Padding}">
                          <Grid Background="white" Height="20"  x:Name="Grid" SnapsToDevicePixels="False">
                            <Grid.ColumnDefinitions>
                              <ColumnDefinition Width="19"/>
                              <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <Path Stroke="black" StrokeThickness="2" HorizontalAlignment="Center" VerticalAlignment="Center" x:Name="arrow" SnapsToDevicePixels="False" Data="M1,1.5 L4.5,5 8,1.5"/>
                            <ContentPresenter DataContext="{x:Null}" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="4,0,0,0" Grid.Column="1" x:Name="ContentSite" Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}" ContentTemplateSelector="{TemplateBinding ContentTemplateSelector}" RecognizesAccessKey="True"/>
                          </Grid>
                        </Border>
                        <ControlTemplate.Triggers>
                          <Trigger Property="IsChecked" Value="True">
                            <Setter Property="Data" Value="M1,4.5 L4.5,1 8,4.5" TargetName="arrow"/>
                          </Trigger>
                          <Trigger Property="IsMouseOver" Value="True">

                            <Setter Property="Stroke" Value="#FF222222" TargetName="arrow"/>

                          </Trigger>
                        </ControlTemplate.Triggers>
                      </ControlTemplate>
                    </Setter.Value>
                  </Setter>
                </Style>
              </ToggleButton.Style>
            </ToggleButton>
            <ContentPresenter    DockPanel.Dock="Bottom" x:Name="ExpandSite" RenderTransformOrigin="0.5,0.5"  >
              <ContentPresenter.LayoutTransform>
                <TransformGroup >
                  <ScaleTransform x:Name="LayoutScaleTransform" ScaleX="1" ScaleY="1" />
                </TransformGroup>
              </ContentPresenter.LayoutTransform>
            </ContentPresenter>
          </DockPanel>
        </Border>
        <ControlTemplate.Triggers>
          <Trigger Property="IsExpanded" Value="False">
            <Trigger.EnterActions>
              <BeginStoryboard>
                <Storyboard>
                  <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetProperty="(ScaleTransform.ScaleY)" Storyboard.TargetName="LayoutScaleTransform" AccelerationRatio="1">
                    <SplineDoubleKeyFrame d:KeyEase="Linear;Linear;0.5;0.5;0.5;0.5" KeySpline="0.5,0.5,0.5,0.5" Value="0" KeyTime="00:00:00"/>
                    <SplineDoubleKeyFrame d:KeyEase="Linear;Linear;0.5;0.5;0.5;0.5" KeySpline="0.5,0.5,0.5,0.5" Value="1" KeyTime="00:00:00.25"/>
                  </DoubleAnimationUsingKeyFrames>
                </Storyboard>
              </BeginStoryboard>
            </Trigger.EnterActions>
            <Trigger.ExitActions>
              <BeginStoryboard>
                <Storyboard>
                  <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetProperty="(ScaleTransform.ScaleY)" Storyboard.TargetName="LayoutScaleTransform" DecelerationRatio="1">
                    <SplineDoubleKeyFrame d:KeyEase="Linear;Linear;0.5;0.5;0.5;0.5" KeySpline="0.5,0.5,0.5,0.5" Value="1" KeyTime="00:00:00"/>
                    <SplineDoubleKeyFrame d:KeyEase="Linear;Linear;0.5;0.5;0.5;0.5" KeySpline="0.5,0.5,0.5,0.5" Value="00" KeyTime="00:00:00.25"/>
                  </DoubleAnimationUsingKeyFrames>
                </Storyboard>
              </BeginStoryboard>
            </Trigger.ExitActions>
          </Trigger>
        </ControlTemplate.Triggers>

      </ControlTemplate>
      <ControlTemplate x:Key="CustomExpander" TargetType="{x:Type Expander}">
        <Border Background="{TemplateBinding Background}" x:Name="Border" SnapsToDevicePixels="True" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Padding="{TemplateBinding Padding}" CornerRadius="3,3,3,3">
          <DockPanel x:Name="DockPanel">
            <ToggleButton IsChecked="{Binding Path=IsExpanded,Mode=TwoWay,RelativeSource={RelativeSource TemplatedParent}}" FontSize="{TemplateBinding FontSize}" FontFamily="{TemplateBinding FontFamily}" FontStyle="{TemplateBinding FontStyle}" FontWeight="{TemplateBinding FontWeight}" Margin="1,1,1,1" DockPanel.Dock="Top" MinWidth="0" MinHeight="0" x:Name="HeaderSite" Content="{TemplateBinding Header}" ContentTemplate="{TemplateBinding HeaderTemplate}" ContentTemplateSelector="{TemplateBinding HeaderTemplateSelector}" HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}" FontStretch="{TemplateBinding FontStretch}" VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}" Foreground="{TemplateBinding Foreground}" Padding="{TemplateBinding Padding}">
              <ToggleButton.Style>
                <Style TargetType="{x:Type ToggleButton}">
                  <Setter Property="Template">
                    <Setter.Value>
                      <ControlTemplate TargetType="{x:Type ToggleButton}">
                        <Border x:Name="Border" Padding="{TemplateBinding Padding}">
                          <Grid Background="{DynamicResource HeaderBackgroundGradient1}" Height="20"  x:Name="Grid" SnapsToDevicePixels="False">
                            <Grid.ColumnDefinitions>
                              <ColumnDefinition Width="19"/>
                              <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <Path Stroke="white" StrokeThickness="2" HorizontalAlignment="Center" VerticalAlignment="Center" x:Name="arrow" SnapsToDevicePixels="False" Data="M1,1.5 L4.5,5 8,1.5"/>
                            <ContentPresenter DataContext="{x:Null}" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="4,0,0,0" Grid.Column="1" x:Name="ContentSite" Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}" ContentTemplateSelector="{TemplateBinding ContentTemplateSelector}" RecognizesAccessKey="True"/>
                          </Grid>
                        </Border>
                        <ControlTemplate.Triggers>
                          <Trigger Property="IsChecked" Value="True">
                            <Setter Property="Data" Value="M1,4.5 L4.5,1 8,4.5" TargetName="arrow"/>
                          </Trigger>
                          <Trigger Property="IsMouseOver" Value="True">

                            <Setter Property="Stroke" Value="#FF222222" TargetName="arrow"/>

                          </Trigger>
                        </ControlTemplate.Triggers>
                      </ControlTemplate>
                    </Setter.Value>
                  </Setter>
                </Style>
              </ToggleButton.Style>

            </ToggleButton>
            <Grid>
              <ContentPresenter Visibility="Collapsed" DataContext="{x:Null}" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}" Margin="{TemplateBinding Padding}" Focusable="False" DockPanel.Dock="Bottom" x:Name="ExpandSite" Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}" ContentTemplateSelector="{TemplateBinding ContentTemplateSelector}"/>
            </Grid>
          </DockPanel>
        </Border>
        <ControlTemplate.Triggers>
          <Trigger Property="IsExpanded" Value="True">
            <Setter Property="Visibility" Value="Visible" TargetName="ExpandSite"/>
          </Trigger>



        </ControlTemplate.Triggers>
      </ControlTemplate>

      <Style x:Key="ButtonStyle1" TargetType="{x:Type Button}" BasedOn="{x:Null}">
        <Setter Property="Template">
          <Setter.Value>
            <ControlTemplate TargetType="{x:Type Button}">
              <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="0,0,0,0" x:Name="Grid" RenderTransformOrigin="0.5,0.5" SnapsToDevicePixels="True">
                <Grid.ColumnDefinitions>
                  <ColumnDefinition/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                  <RowDefinition/>
                </Grid.RowDefinitions>
                <Rectangle  Fill="#cccccc" RadiusX="3" RadiusY="3" Margin="0,0,0,0" x:Name="StrokeRectangle" RenderTransformOrigin="0.5,0.5" StrokeThickness="2">
                  <Rectangle.Stroke>
                    <LinearGradientBrush StartPoint="0,0.499999" EndPoint="1,0.499999">
                      <LinearGradientBrush.GradientStops>
                        <GradientStopCollection>
                          <GradientStop Color="#FFCBCED0" Offset="0"/>
                          <GradientStop Color="#FFE6E7E9" Offset="0.365168"/>
                          <GradientStop Color="#FFB9BBBD" Offset="1"/>
                        </GradientStopCollection>
                      </LinearGradientBrush.GradientStops>
                    </LinearGradientBrush>
                  </Rectangle.Stroke>
                </Rectangle>
                <Rectangle  Stroke="transparent" RadiusX="3" RadiusY="3" VerticalAlignment="Stretch" Margin="2.25,3,2.25,3" Width="Auto" Height="Auto" x:Name="FillRectangle" RenderTransformOrigin="0.5,0.5">
                  <Rectangle.Fill>
                    <LinearGradientBrush StartPoint="0,0.499999" EndPoint="1,0.499999">
                      <LinearGradientBrush.GradientStops>
                        <GradientStopCollection>
                          <GradientStop Color="#FF7C848A" Offset="0"/>
                          <GradientStop Color="#FFC0C2C7" Offset="0.365168"/>
                          <GradientStop Color="#FF505559" Offset="1"/>
                        </GradientStopCollection>
                      </LinearGradientBrush.GradientStops>
                      <LinearGradientBrush.RelativeTransform>
                        <TransformGroup>
                          <TranslateTransform X="-0.5" Y="-0.499999"/>
                          <ScaleTransform ScaleX="1" ScaleY="1"/>
                          <SkewTransform AngleX="0" AngleY="0"/>
                          <RotateTransform Angle="90"/>
                          <TranslateTransform X="0.5" Y="0.499999"/>
                          <TranslateTransform X="0" Y="-5.5511151231257827E-17"/>
                        </TransformGroup>
                      </LinearGradientBrush.RelativeTransform>
                    </LinearGradientBrush>
                  </Rectangle.Fill>
                </Rectangle>

                <TextBlock Text="{TemplateBinding Content}" TextAlignment="center" VerticalAlignment="center" Foreground="black" Margin="0,0,-1.5,-1.5"  />
                <TextBlock Text="{TemplateBinding Content}" TextAlignment="center" VerticalAlignment="center" Foreground="white"  />
              </Grid>
              <ControlTemplate.Triggers>
                <Trigger Property="IsFocused" Value="True"/>
                <Trigger Property="IsDefaulted" Value="True"/>
                <Trigger Property="IsMouseOver" Value="True">
                  <Setter Property="Shape.Fill" TargetName="FillRectangle">
                    <Setter.Value>
                      <LinearGradientBrush StartPoint="0,0.499999" EndPoint="1,0.499999">
                        <LinearGradientBrush.RelativeTransform>
                          <TransformGroup>
                            <TranslateTransform X="-0.5" Y="-0.499999"/>
                            <ScaleTransform ScaleX="1" ScaleY="1"/>
                            <SkewTransform AngleX="0" AngleY="0"/>
                            <RotateTransform Angle="90"/>
                            <TranslateTransform X="0.5" Y="0.499999"/>
                            <TranslateTransform X="0" Y="-5.5511151231257827E-17"/>
                          </TransformGroup>
                        </LinearGradientBrush.RelativeTransform>
                        <LinearGradientBrush.GradientStops>
                          <GradientStop Color='#FF103148' Offset='0'/>
                          <GradientStop Color='#FF3E9FC4' Offset='0.52809'/>
                          <GradientStop Color='#FF6CE7FD' Offset='1'/>
                        </LinearGradientBrush.GradientStops>
                      </LinearGradientBrush>
                    </Setter.Value>
                  </Setter>
                  <Setter Property="Shape.Fill" TargetName="StrokeRectangle" Value="#FF3E9FC4">

                  </Setter>
                  <Setter Property="Shape.Stroke" TargetName="StrokeRectangle">
                    <Setter.Value>
                      <LinearGradientBrush StartPoint="0,0.499999" EndPoint="1,0.499999">
                        <LinearGradientBrush.RelativeTransform>
                          <TransformGroup>
                            <TranslateTransform X="-0.5" Y="-0.499999"/>
                            <ScaleTransform ScaleX="1" ScaleY="1"/>
                            <SkewTransform AngleX="0" AngleY="0"/>
                            <RotateTransform Angle="90"/>
                            <TranslateTransform X="0.5" Y="0.499999"/>
                            <TranslateTransform X="0" Y="-5.5511151231257827E-17"/>
                          </TransformGroup>
                        </LinearGradientBrush.RelativeTransform>
                        <LinearGradientBrush.GradientStops>
                          <GradientStop Color='#FF103148' Offset='0'/>
                          <GradientStop Color='#FF3E9FC4' Offset='0.52809'/>
                          <GradientStop Color='#FF6CE7FD' Offset='1'/>
                        </LinearGradientBrush.GradientStops>
                      </LinearGradientBrush>
                    </Setter.Value>
                  </Setter>
                </Trigger>
                <Trigger Property="IsPressed" Value="True"/>
                <Trigger Property="IsEnabled" Value="False"/>
              </ControlTemplate.Triggers>
            </ControlTemplate>
          </Setter.Value>
        </Setter>
      </Style>
      <Storyboard x:Key="OnLoaded"/>
      <ToolTip x:Key="Label3DToolTip">
        <Grid VerticalAlignment="Stretch"   Background="#FFFFFFFF" >
          <Image  Source="2dgraphmsft.gif" RenderTransformOrigin="0.5,0.5"/>
        </Grid>
      </ToolTip>
      <!-- <XmlDataProvider x:Key="StockNewsDataSource" d:IsDataSource="True" Source="http://finance.yahoo.com/rss/headline?s=MSFT"/> -->
      <DataTemplate x:Key="itemTemplate">
        <StackPanel Margin="10,5,0,5" Width="207.72" x:Name="StackPanel">

          <TextBlock FontSize="12" FontWeight="Normal" x:Name="TextBlock1" Text="{Binding Mode=OneWay, XPath=title}" TextTrimming="WordEllipsis" Foreground="#253C85" FontFamily="Segoe UI"/>
          <TextBlock x:Name="TextBlock" Text="{Binding Mode=OneWay, XPath=pubDate}" Foreground="#A3A4A6" FontSize="10" FontFamily="Segoe UI"/>
        </StackPanel>
      </DataTemplate>
      <LinearGradientBrush x:Key="HeaderBackgroundGradient1" StartPoint='0.5,0' EndPoint='0.5,1'>
        <LinearGradientBrush.GradientStops>
          <GradientStop Color='#FF828487' Offset='0.365168'/>
          <GradientStop Color='#FF505559' Offset='1'/>
        </LinearGradientBrush.GradientStops>
      </LinearGradientBrush>
      <Style x:Key="{x:Type TextBlock}" TargetType="{x:Type TextBlock}">
        <Setter Property="TextWrapping" Value="NoWrap"/>
        <Setter Property="TextTrimming" Value="None"/>
        <Setter Property="FontFamily" Value="Segoe UI" />
      </Style>

      <XmlDataProvider x:Key="PortfolioDataSource" d:IsDataSource="True" Source="pack://application:,,,//FinanceExplorer//Equity.xml"/>

      <Style x:Key="StockListBoxItem" TargetType="{x:Type WoodgroveFinanceStockChartFrame:StockListBoxItem}">
        <Setter Property="Cursor" Value="Hand" />
        <Setter Property="Padding" Value="0" />
        <Style.Triggers>
          <DataTrigger Binding="{Binding XPath=Summary/Change, Converter={StaticResource IsNegativeConverterReference}}" Value="True">
            <Setter Property="Foreground" Value="Red" />
            <Setter Property="WoodgroveFinanceStockChartFrame:StockListBoxItem.IsNegative" Value="True" />
          </DataTrigger>
          <DataTrigger Binding="{Binding XPath=Summary/Change, Converter={StaticResource IsNegativeConverterReference}}" Value="False">
            <Setter Property="Foreground" Value="Green" />
            <Setter Property="WoodgroveFinanceStockChartFrame:StockListBoxItem.IsNegative" Value="False" />
          </DataTrigger>

          <Trigger Property="WoodgroveFinanceStockChartFrame:StockListBoxItem.IsEven" Value="True">
            <Setter Property="Background" Value="#e4e4e4" />
          </Trigger>
          <Trigger Property="WoodgroveFinanceStockChartFrame:StockListBoxItem.IsEven" Value="False">
            <Setter Property="Background" Value="white" />
          </Trigger>

          <Trigger Property="WoodgroveFinanceStockChartFrame:StockListBoxItem.IsMouseHoverSelected" Value="True">
            <Setter Property="Background" >
              <Setter.Value>
                <LinearGradientBrush StartPoint="0,0" EndPoint="0,1" Opacity="1" >
                  <GradientBrush.GradientStops>
                    <GradientStopCollection >
                      <GradientStop Color="DarkGreen" Offset="0" />
                      <GradientStop Color="Green" Offset="1" />
                    </GradientStopCollection>
                  </GradientBrush.GradientStops>
                </LinearGradientBrush>
              </Setter.Value>
            </Setter>
            <Setter Property="Foreground"
        Value="White"/>
          </Trigger>

          <Trigger Property="WoodgroveFinanceStockChartFrame:StockListBoxItem.IsItemSelected" Value="True">
            <Setter Property="Background" >
              <Setter.Value>
                <LinearGradientBrush StartPoint="0,0" EndPoint="0,1" Opacity="1" >
                  <GradientBrush.GradientStops>
                    <GradientStopCollection >
                      <GradientStop Color="Black" Offset="0" />
                      <GradientStop Color="Green" Offset="1" />
                    </GradientStopCollection>
                  </GradientBrush.GradientStops>
                </LinearGradientBrush>
              </Setter.Value>
            </Setter>
            <Setter Property="Foreground"
        Value="White"/>
          </Trigger>

          <MultiTrigger>
            <MultiTrigger.Conditions>
              <Condition Property="WoodgroveFinanceStockChartFrame:StockListBoxItem.IsMouseHoverSelected"
                         Value="True"/>
              <Condition Property="WoodgroveFinanceStockChartFrame:StockListBoxItem.IsNegative"
                         Value="True"/>
            </MultiTrigger.Conditions>
            <Setter Property="Background">
              <Setter.Value>
                <LinearGradientBrush StartPoint="0,0" EndPoint="0,1" Opacity="1" >
                  <GradientBrush.GradientStops>
                    <GradientStopCollection >
                      <GradientStop Color="Maroon" Offset="0" />
                      <GradientStop Color="Red" Offset="1" />
                    </GradientStopCollection>
                  </GradientBrush.GradientStops>
                </LinearGradientBrush>
              </Setter.Value>
            </Setter>
            <Setter Property="Foreground"
        Value="White"/>
          </MultiTrigger>
          <MultiTrigger>
            <MultiTrigger.Conditions>
              <Condition Property="WoodgroveFinanceStockChartFrame:StockListBoxItem.IsItemSelected"
                         Value="True"/>
              <Condition Property="WoodgroveFinanceStockChartFrame:StockListBoxItem.IsNegative"
                         Value="True"/>
            </MultiTrigger.Conditions>
            <Setter Property="Background">
              <Setter.Value>
                <LinearGradientBrush StartPoint="0,0" EndPoint="0,1" Opacity="1" >
                  <GradientBrush.GradientStops>
                    <GradientStopCollection >
                      <GradientStop Color="Black" Offset="0" />
                      <GradientStop Color="Red" Offset="1" />
                    </GradientStopCollection>
                  </GradientBrush.GradientStops>
                </LinearGradientBrush>
              </Setter.Value>
            </Setter>
            <Setter Property="Foreground"
                    Value="White"/>
          </MultiTrigger>
        </Style.Triggers>


      </Style>


      <DataTemplate x:Key="EquityTemplate">
        <Grid>
          <Rectangle Margin="2" >
            <Rectangle.Fill>
              <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                <LinearGradientBrush.GradientStops>
                  <GradientStopCollection>
                    <GradientStop Color="#aaffffff" Offset="0"/>
                    <GradientStop Color="transparent" Offset="1"/>
                  </GradientStopCollection>
                </LinearGradientBrush.GradientStops>
              </LinearGradientBrush>
            </Rectangle.Fill>
          </Rectangle>
          <Grid x:Name="StackPanel1" Height="29.1333333333334" Margin="5" >
            <Grid.ColumnDefinitions>
              <ColumnDefinition Width="71"/>
              <ColumnDefinition Width="56"/>
              <ColumnDefinition Width="50.2107754545661"/>
            </Grid.ColumnDefinitions>
            <TextBlock FontFamily="Segoe UI"    x:Name="TextBlock1" Grid.Column="0"  Width="Auto"  Text="{Binding Mode=OneWay, XPath=Symbol}" Margin="-0.5,0,15.5,12.4666666666666" FontSize="12" FontWeight="Bold" VerticalAlignment="Stretch" Height="Auto"/>
            <TextBlock  FontFamily="Segoe UI" x:Name="TextBlock2"  Grid.Column="1" Width="20"  Text="{Binding Mode=OneWay, XPath=Shares}" d:LayoutOverrides="None" FontSize="10" VerticalAlignment="Stretch" Margin="0,0,3.9902809169099,14.4666666666666" Height="Auto" Grid.ColumnSpan="1"/>
            <TextBlock  FontFamily="Segoe UI" x:Name="TextBlock" Grid.Column="2"  Width="28"  Text="{Binding Mode=OneWay, XPath=TotalValue}" FontSize="10" TextAlignment="Left" FontStyle="Normal" FontWeight="Bold" VerticalAlignment="Bottom" Margin="0,0,0,0" Height="15.6666666666668" HorizontalAlignment="Right"/>
            <TextBlock  FontFamily="Segoe UI" Margin="0,0,0,0" Width="Auto" Height="16.6666666666668" Grid.Column="0" Grid.ColumnSpan="1" x:Name="TextBlock1_Copy" Text="{Binding Mode=OneWay, XPath=Name}"  d:LayoutOverrides="GridBox" FontSize="10" VerticalAlignment="Bottom" HorizontalAlignment="Stretch" TextTrimming="WordEllipsis"/>
            <TextBlock FontFamily="Segoe UI"  d:LayoutOverrides="None" FontSize="10"  FontWeight="Bold" VerticalAlignment="Stretch" Margin="0,14.401028277635,0,0.065638389031637" Width="26" Height="Auto" Grid.Column="1" Grid.ColumnSpan="1" x:Name="TextBlock2_Copy" Text="{Binding Mode=OneWay, XPath=Summary/LastTrade}"  HorizontalAlignment="Center"/>
          </Grid>
        </Grid>
      </DataTemplate>

      <Style x:Key="SummaryHorizontalStack1" TargetType="{x:Type StackPanel}">
        <Setter Property="Height" Value="20" />
        <Setter Property="Orientation" Value="Horizontal" />
        <Setter Property="Background" Value="#ffffff" />
      </Style>
      <Style x:Key="SummaryHorizontalStack2" TargetType="{x:Type StackPanel}">
        <Setter Property="Height" Value="20" />
        <Setter Property="Orientation" Value="Horizontal" />
        <Setter Property="Background" Value="#e4e4e4" />
      </Style>
      <Style x:Key="SummaryTextLable" TargetType="{x:Type TextBlock}">
        <Setter Property="FontWeight" Value="Bold" />
        <Setter Property="Width" Value="100" />
        <Setter Property="Margin" Value="5" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="FontFamily" Value="Segoe UI" />
      </Style>
      <Style x:Key="SummaryDataTextLable" TargetType="{x:Type TextBlock}">
        <Setter Property="TextTrimming" Value="WordEllipsis" />
        <Setter Property="Width" Value="75" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="FontFamily" Value="Segoe UI" />
      </Style>


      <DataTemplate x:Key="SelectedSummaryTemplate">

        <StackPanel>
          <DockPanel d:LayoutOverrides="Height" VerticalAlignment="Top" Margin="0,0,0,0" Width="Auto" Height="25" Background="{DynamicResource HeaderBackgroundGradient1}" x:Name="SummaryStockNameGrid">

            <Grid Height="18" Width="18" DockPanel.Dock="right" Margin="0,0,5,0" >
              <Ellipse    x:Name="StatusPearl" >
                <Ellipse.Stroke>
                <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                  <LinearGradientBrush.GradientStops>
                    <GradientStopCollection>
                      <GradientStop Color="black" Offset="0"/>
                      <GradientStop Color="white" Offset="1"/>
                    </GradientStopCollection>
                  </LinearGradientBrush.GradientStops>
                </LinearGradientBrush>
              </Ellipse.Stroke>
            </Ellipse>
              <Ellipse Height="10" VerticalAlignment="Top" Margin="3,1,3,0"  >
                <Ellipse.Fill>
                  <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                    <LinearGradientBrush.GradientStops>
                      <GradientStopCollection>
                        <GradientStop Color="#ccffffff" Offset="0"/>
                        <GradientStop Color="transparent" Offset="1"/>
                      </GradientStopCollection>
                    </LinearGradientBrush.GradientStops>
                  </LinearGradientBrush>
                </Ellipse.Fill>
              </Ellipse>
          </Grid>
            <TextBlock   Text="{Binding XPath=Symbol}" FontSize="13" FontFamily="Segoe UI" FontWeight="Normal" Margin="5,4,2,5" RenderTransformOrigin="0.5,0.5" Foreground="#ffffff" TextWrapping="Wrap" TextAlignment="Right" />
            <TextBlock   Text="-" FontSize="13" FontFamily="Segoe UI" FontWeight="Bold" Margin="0,5,2,5" RenderTransformOrigin="0.5,0.5" Foreground="#ffffff" TextWrapping="Wrap" Width="5" Height="15" />
            <TextBlock Width="Auto"  Margin="0,3,5,5" Text="{Binding XPath=../Name}" FontSize="13" FontFamily="Segoe UI" FontWeight="Normal"  TextTrimming="WordEllipsis"  Foreground="#ffffff"  Height="15"  />
          </DockPanel>
          <Grid d:LayoutOverrides="Height" VerticalAlignment="Stretch" Margin="0,0,0,5"  Background="#FFFFFFFF" x:Name="StockChart2DGrid" RenderTransformOrigin="0.5,0.5">

            <TextBlock TextAlignment="Center" VerticalAlignment="center" FontSize="20pt"  Foreground="#999999">No Chart</TextBlock>
            <Image x:Name="Chart2DImage" Source="{Binding XPath=../Chart2D/@SmallPic,Converter={StaticResource stockImageConverter}}" RenderTransformOrigin="0.5,0.5" Margin="5,10,5,5">
              <Image.ToolTip>
                <Image x:Name="Chart2DToolTipImage" Source="{Binding XPath=../Chart2D/@LargePic,Converter={StaticResource stockImageConverter}}" />
              </Image.ToolTip>
            </Image>
            <Rectangle d:LayoutOverrides="Height, Margin" Stroke="{x:Null}" VerticalAlignment="Top" Width="Auto" Height="7" x:Name="Rectangle" RenderTransformOrigin="0.5,0.5">
              <Rectangle.Fill>
                <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                  <LinearGradientBrush.GradientStops>
                    <GradientStopCollection>
                      <GradientStop Color="#FF828487" Offset="0"/>
                      <GradientStop Color="sc#0, 0.08021982, 0.09084171, 0.0998987257" Offset="1"/>
                    </GradientStopCollection>
                  </LinearGradientBrush.GradientStops>
